{
  "name": "jsonpathly",
  "version": "1.0.0",
  "description": "Query json with jsonpath expressions",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "coverage": "jest --config jestconfig.json --coverage",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atamano/jsonpathly.git"
  },
  "keywords": [
    "jsonpath",
    "json-path",
    "json",
    "path"
  ],
  "author": "Antoine TAMANO",
  "license": "Apache License, Version 2.0, January 2004",
  "bugs": {
    "url": "https://github.com/atamano/jsonpathly/issues"
  },
  "homepage": "https://github.com/atamano/jsonpathly#readme",
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "antlr4ts-cli": "^0.5.0-alpha.4",
    "jest": "^27.5.1",
    "jison": "^0.4.18",
    "prettier": "^2.6.2",
    "ts-jest": "^27.1.4",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.6.3"
  },
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "antlr4ts": "^0.5.0-alpha.4",
    "lodash.isplainobject": "^4.0.6"
  }
}
