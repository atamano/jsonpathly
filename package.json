{
  "name": "jsonpathly",
  "version": "1.7.1",
  "description": "Query json with jsonpath expressions",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest --config jestconfig.json",
    "ci:test": "jest --config jestconfig.json --runInBand --ci --coverage --coverageDirectory=coverage",
    "build": "rollup -c rollup.config.js",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint src --ext .ts",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atamano/jsonpathly.git"
  },
  "keywords": [
    "jsonpath",
    "typescript",
    "json",
    "path"
  ],
  "author": "Antoine Tamano",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/atamano/jsonpathly/issues"
  },
  "homepage": "https://github.com/atamano/jsonpathly#readme",
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-transform-modules-commonjs": "^7.17.9",
    "@babel/plugin-transform-react-jsx": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.2",
    "@types/antlr4": "^4.7.2",
    "@types/jest": "^27.5.0",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.0.0",
    "babel-loader": "8.2.3",
    "core-js": "^3.22.5",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^27.5.1",
    "lint-staged": "^10.0.8",
    "prettier": "^2.6.2",
    "rollup": "^2.72.1",
    "rollup-plugin-node-builtins": "^2.1.2",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "antlr4": "^4.10.1"
  }
}
