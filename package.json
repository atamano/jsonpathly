{
  "name": "jsonpathly",
  "version": "3.0.0",
  "description": "Query json with jsonpath expressions",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "gen": "peggy --format es -o src/parser/generated/parser.js src/parser/jsonpath.peggy",
    "test": "mocha --require ts-node/register --timeout 60000 --exit --no-cache tests/*.ts",
    "build": "tsup",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint src --ext .ts",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm test && npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "release:patch": "npm version patch -m 'chore: release v%s'",
    "release:minor": "npm version minor -m 'chore: release v%s'",
    "release:major": "npm version major -m 'chore: release v%s'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/atamano/jsonpathly.git"
  },
  "keywords": [
    "jsonpath",
    "typescript",
    "json",
    "path"
  ],
  "author": "Antoine Tamano",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/atamano/jsonpathly/issues"
  },
  "homepage": "https://github.com/atamano/jsonpathly#readme",
  "devDependencies": {
    "@types/chai": "^4.3.6",
    "@types/mocha": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "c8": "^10.1.3",
    "chai": "^4.3.8",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "lint-staged": "^10.0.8",
    "mocha": "^10.2.0",
    "peggy": "^5.0.6",
    "prettier": "^2.6.2",
    "ts-node": "^10.9.1",
    "tsup": "^8.5.1",
    "typescript": "^4.9.5"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    }
  }
}
