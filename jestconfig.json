{
  "transform": {
    "^.+\\.js$": "babel-jest",
    "^.+\\.ts$": "ts-jest"
  },
  "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js|ts)$",
  "modulePathIgnorePatterns": ["dist"],
  "transformIgnorePatterns": ["node_modules/(?!antlr4/.*)"],
  "coveragePathIgnorePatterns": ["parser/generated"],
  "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"]
}
